package Assignment;

//*  Compute the sum of the first 100 prime numbers. *//

public class Sum_primenumber {
	public static void main(String[] args) {
		int num = 12;
		int temp = 0;
		for(int i=2; i<=num-1; i++) 
		{
			if(num%i==0)
			{
				temp = temp+1;
				
			}	
		}
		if(temp==0)
		{
			System.out.println("Num is prime");
		}
		else
		{
			System.out.println("Num is not prime");
		}
	}
}
