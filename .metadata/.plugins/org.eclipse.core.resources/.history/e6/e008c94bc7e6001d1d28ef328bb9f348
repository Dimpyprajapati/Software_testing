package core;

public class sum_two_array {
	public static void main(String args[]) {
		int num1[][] = {{1,3,4},{2,4,3},{3,4,5}};
		int num2[][] = {{1,3,4},{2,4,3},{1,2,4}};
		int sum[][] = {{0,0,0},{0,0,0}};
		
		for(int i=0; i<num1[i].length; i++) {
			for(int j=0; j<num1[j].length; j++) {
				System.out.println(i+ " " +j);
				sum[i][j] = num1[i][j] + num2[i][j];
			}
	    }
		for(int i=0; i<num1[i].length; i++) {
			for(int j=0; j<num1[j].length; j++) {
				System.out.println(sum[i][j] + " ");
				sum[i][j] = num1[i][j] + num2[i][j];
			}
			System.out.println();
		}
	}
}
