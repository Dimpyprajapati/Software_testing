package core;

import java.util.Scanner;

//1. for;
//2. while;
//3. do while;
//4. for each;

//for  (initialization;condition;increment,decrement) {
	// printing statement
	// }
//while (condition){
      // printing statement
      // }
		
public class loop {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter num : ");
		int num = sc.nextInt();
		for (int i=0; i<=10; i++) {
			System.out.println(num+"x" + i + "=" + num*i);
		}
		
	}

}
