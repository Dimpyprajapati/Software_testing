//5. switch case;
	

import java.util.Scanner;

public class conditionalstatement {
	public static void main(String args[]) {
		int a ,b ,c;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter value of a=");
		a= sc.nextInt();
		System.out.println("Enter value of b=");
		b= sc.nextInt();
		
	}
	

}
